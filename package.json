{
  "name": "lhasa-ligand-builder",
  "description": "Moorhen's ligand builder: React + WebAssembly version of Layla - Coot's ligand builder",
  "version": "0.2.1",
  "type": "module",
  "main": "dist/lhasa-ligand-builder.js",
  "module": "dist/lhasa-ligand-builder.js",
  "types": "dist/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lhasa-ligand-builder.js",
      "types": "./dist/lib.d.ts"
    },
    "./style.css": "./dist/lhasa-ligand-builder.css",
    "./lhasa-vite-plugin": "./bundler_plugins/vite.mjs",
    "./lhasa-webpack-plugin": "./bundler_plugins/webpack.cjs"
  },
  "files": [
    "dist",
    "bundler_plugins"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "BUILD_MODE=lib vite build && node scripts/copy-assets.mjs",
    "prepublishOnly": "npm run build:lib",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.2",
    "@mui/material": "^6.4.2",
    "d3": "^7.9.0",
    "react-hotkeys": "^2.0.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/node": "^25.2.3",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.83.4",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-cross-origin-isolation": "^0.1.6",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-top-level-await": "^1.4.4"
  }
}
